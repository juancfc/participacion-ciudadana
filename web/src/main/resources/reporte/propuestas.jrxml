	<queryString>
		<![CDATA[SELECT
	PRO.ID AS idPropuesta,
	PER.ID AS idPersona,
	PER.NOMBRE,
	PER.PATERNO,
	PER.MATERNO,
	PER.CORREO_ELECTRONICO,
	PER.TELEFONO,
	LC.LOCALIDAD,
	MN.MUNICIPIO,
	PRO.PROPUESTA1,
	PRO.PROPUESTA2,
	PRO.PROPUESTA3,
	PRO.PROPUESTA4
FROM
	PROPUESTA PRO,
	PERSONA PER,
	LOCALIDAD LC,
	MUNICIPIO MN
WHERE
	PRO.PERSONA_ID=PER.ID
	AND PER.LOCALIDAD_ID=LC.ID
	AND LC.MUNICIPIO_ID=MN.ID
	AND MN.ID= $P{municipioid}]]>
